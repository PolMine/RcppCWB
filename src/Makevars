PKG_CPPFLAGS=-I/Users/blaette/Lab/github/RcppCWB/inst/include/cwb -I/Users/blaette/Lab/github/RcppCWB/src/cwb/cqp -I/Users/blaette/Lab/github/RcppCWB/src/cwb/cl -I/Users/blaette/Lab/github/RcppCWB/src/cwb/CQi
PKG_LIBS=-L/Users/blaette/Lab/github/RcppCWB/src/cwb/cl -L/Users/blaette/Lab/github/RcppCWB/src/cwb/cqp -lglib-2.0 -lpcre -lcl -lcqp

$(SHLIB): libcl.a libcqp.a

libcl.a: depend
	R_PACKAGE_SOURCE=/Users/blaette/Lab/github/RcppCWB/src/cwb $(MAKE) -C cwb cl

libcqp.a: depend
	R_PACKAGE_SOURCE=/Users/blaette/Lab/github/RcppCWB/src/cwb $(MAKE) -C cwb cqp

depend: clean
	R_PACKAGE_SOURCE=/Users/blaette/Lab/github/RcppCWB/src/cwb $(MAKE) -C cwb depend

clean:
	R_PACKAGE_SOURCE=/Users/blaette/Lab/github/RcppCWB/src/cwb $(MAKE) -C cwb clean

