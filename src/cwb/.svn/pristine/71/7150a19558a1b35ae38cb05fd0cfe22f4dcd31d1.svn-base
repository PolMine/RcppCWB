<a class="anchor" id="a0ae0e3bf05524de6e1a4be2d4cbc8000"></a><!-- doxytag: member="cwb&#45;decode&#45;nqrfile.c::nqrfile_print_info" ref="a0ae0e3bf05524de6e1a4be2d4cbc8000" args="(FILE *fd, int print_header)" -->
<div class="memitem">
<div class="memproto">
      <table class="memname">
        <tr>
          <td class="memname">int nqrfile_print_info </td>
          <td>(</td>
          <td class="paramtype">FILE *&#160;</td>
          <td class="paramname"><em>fd</em>, </td>
        </tr>
        <tr>
          <td class="paramkey"></td>
